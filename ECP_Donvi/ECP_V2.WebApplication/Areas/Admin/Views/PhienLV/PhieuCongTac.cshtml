<script>
    setTimeout(function () {
        if (document.location.href.indexOf("#print") > -1) {
            $(".panel-footer").hide();
            //$("#tb4 .selectboxv1").hide();
            $("#txtTop_LoaiCviec").hide();
            $("#txtTop_Tram").hide();

            @if (!string.IsNullOrEmpty(phieuCongTac.NguoiDuyet))
                {
            @:$("#btnPrint").show();
                }
            $("#btnSaveTable").show();
              //$("#divMaYCKH").hide();
        }
    }, 1000);

    function RunPrint() {
        console.log("");
        $("#btnPrint").hide();
          $("#btnSaveTable").hide();
        window.print();
        $("#btnPrint").show();
         $("#btnSaveTable").show();
    }

    $(function () {
        var startX,
            startWidth,
            $handle,
            $table,
            pressed = false;

        $(document).on({
            mousemove: function (event) {
                if (pressed) {
                    $handle.width(startWidth + (event.pageX - startX));
                }
            },
            mouseup: function () {
                if (pressed) {
                    $table.removeClass('resizing');
                    pressed = false;
                }
            }
        }).on('mousedown', '.table-resizable th', function (event) {
            $handle = $(this);
            pressed = true;
            startX = event.pageX;
            startWidth = $handle.width();

            $table = $handle.closest('.table-resizable').addClass('resizing');
        }).on('dblclick', '.table-resizable thead', function () {
            // Reset column sizes on double click
            $(this).find('th[style]').css('width', '');
        });

        LoadSizeTable();


    });

     function SaveTable() {

        //table 4
        var w = $("#divtb4").width();
        var h = $("#divtb4").height();
        localStorage[@phieuCongTac.ID+'_wdivtb4'] = w;
        localStorage[@phieuCongTac.ID+'_hdivtb4'] = h;

        var i = 1;
        $("#tb4 thead tr th").each(function(){
            var wth = $(this).width();
            var hth = $(this).height();

            localStorage[@phieuCongTac.ID+'_wtb4_th'+i] = wth;
            localStorage[@phieuCongTac.ID+'_htb4_th'+i] = hth;
            i = i +1;
        });

        //table 5
        var w2 = $("#divtb5").width();
        var h2 = $("#divtb5").height();
        localStorage[@phieuCongTac.ID+'_wdivtb5'] = w2;
        localStorage[@phieuCongTac.ID+'_hdivtb5'] = h2;

        var i2 = 1;
        $("#tb5 thead tr th").each(function(){
            var wth2 = $(this).width();
            var hth2 = $(this).height();

            localStorage[@phieuCongTac.ID+'_wtb5_th'+i2] = wth2;
            localStorage[@phieuCongTac.ID+'_htb5_th'+i2] = hth2;
            i2 = i2 +1;
        });

        alert("Lưu thành công");
    }

    function LoadSizeTable() {

        //table 4
        var w = localStorage[@phieuCongTac.ID+'_wdivtb4'] || 0;
        var h = localStorage[@phieuCongTac.ID+'_hdivtb4'] || 0;

        if (w > 0)
            $("#divtb4").css("width", w);
        if (h > 0)
            $("#divtb4").css("height", h);

        var i = 1;
        $("#tb4 thead tr th").each(function () {
            var wth = localStorage[@phieuCongTac.ID+'_wtb4_th' + i] || 0;
            var hth = localStorage[@phieuCongTac.ID+'_htb4_th' + i] || 0;

            if (wth > 0)
                $(this).css("width", wth);
            if (hth > 0)
                $(this).css("height", hth);
            i = i + 1;
        });

        //table 5
        var w2 = localStorage[@phieuCongTac.ID+'_wdivtb5'] || 0;
        var h2 = localStorage[@phieuCongTac.ID+'_hdivtb5'] || 0;

        if (w2 > 0)
            $("#divtb5").css("width", w2);
        if (h2 > 0)
            $("#divtb5").css("height", h2);

        var i2 = 1;
        $("#tb5 thead tr th").each(function () {
            var wth2 = localStorage[@phieuCongTac.ID+'_wtb5_th' + i2] || 0;
            var hth2 = localStorage[@phieuCongTac.ID+'_htb5_th' + i2] || 0;

            if (wth2 > 0)
                $(this).css("width", wth2);
            if (hth2 > 0)
                $(this).css("height", hth2);
            i2 = i2 + 1;
        });

    }

</script>

